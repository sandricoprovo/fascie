{
    "compilerOptions": {
        "target": "es2016",
        "lib": [
            "ESNext"
        ],
        "jsx": "preserve",
        "module": "commonjs",
        "moduleResolution": "node",
        "allowJs": true,
        "sourceMap": true,
        "outDir": "dist",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
    },
    "exclude": [
        "node_modules"
    ]
}